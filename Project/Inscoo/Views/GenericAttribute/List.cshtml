@model IEnumerable<Models.Common.GenericAttributeModel>
<table class="table table-bordered table-header-blur">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Key)
        </th>
     
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KeyGroup)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sequence)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr style="text-align:left">
            <td style="vertical-align:middle">
                @Html.ActionLink(item.Key, "Details", new { id = item.Id })
            </td>
            <td style="vertical-align:middle">
               @{
                   string str = item.Value;
                   string[] d = str.Split(';');
                   foreach (var num in d)
                   {
                    <p>@Html.DisplayFor(modelItem => num)</p>
                   }
                }
            </td>
            <td style="vertical-align:middle">
                @Html.DisplayFor(modelItem => item.KeyGroup)
            </td>
            <td style="vertical-align:middle">
                @Html.DisplayFor(modelItem => item.Sequence)
            </td>
            <td style="vertical-align:middle">
                @Html.DisplayFor(modelItem => item.Description)
            </td>
        </tr>
    }

</table>
@{
    PageCommand command = ViewBag.pageCommand;
    @Html.Pager(command)
}

