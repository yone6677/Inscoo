@model IEnumerable<Domain.CarInsuranceExcel>
<table class="table table-bordered">
    <tr style="background-color:#24A9E4;color:#fff;">
        <th>
            文件名
        </th>
        <th>
            上传时间
        </th>
        <th>
            上传人
        </th>
        <th>
            操作
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Author)
            </td>


            <td>
                <a href="@item.Url">下载</a>
                @if (ViewBag.CanEdit)
                {
                    <span>|</span>
                    @Html.ActionLink("更新", "CarInscuranceCreate", new { excelId = item.Id })
                    <span>|</span>
                    @Html.ActionLink("删除", "CarInscuranceDelete", new { excelId = item.Id })
                }
            </td>



        </tr>
    }
</table>
@{
    PageCommand command = ViewBag.pageCommand;
    @Html.Pager(command)
}
